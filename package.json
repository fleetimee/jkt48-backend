{
    "name": "jkt48-private-message",
    "author": {
        "name": "Novian Andika",
        "email": "novian.andika@bpddiy.co.id",
        "url": "https://fleetime.my.id"
    },
    "version": "1.6.9",
    "main": "src/index.ts",
    "scripts": {
        "dev": "nodemon src/index.ts",
        "clean": "rm -rf dist",
        "build": "tsc && npm run copy-env && npm run copy-drizzle",
        "copy-env": "cpx .env dist/",
        "copy-drizzle": "cpx drizzle dist/drizzle",
        "start": "pm2 start ./dist/index.js --no-daemon",
        "generate": "drizzle-kit generate:pg",
        "generate:custom": "drizzle-kit generate:pg --custom",
        "push": "drizzle-kit push",
        "studio": "drizzle-kit studio",
        "commit": "cz",
        "typecheck": "tsc --noEmit",
        "sentry:sourcemaps": "sentry-cli sourcemaps inject --org novians --project node-express ./dist && sentry-cli sourcemaps upload --org novians --project node-express ./dist"
    },
    "dependencies": {
        "@apple/app-store-server-library": "^1.2.0",
        "@react-email/components": "^0.0.19",
        "@sentry/cli": "^2.31.2",
        "@sentry/node": "^7.113.0",
        "@sentry/profiling-node": "^7.113.0",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "compression": "^1.7.4",
        "cookie": "^0.5.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "date-fns": "^3.6.0",
        "dotenv": "^16.1.4",
        "drizzle-orm": "^0.28.6",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-rate-limit": "^7.1.1",
        "express-status-monitor": "^1.3.4",
        "firebase-admin": "^12.1.0",
        "googleapis": "^140.0.0",
        "helmet": "^7.0.0",
        "http-status-codes": "^2.3.0",
        "jsonwebtoken": "^9.0.1",
        "moment-timezone": "^0.5.45",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nanoid": "^3.3.4",
        "node-apple-receipt-verify": "^1.13.0",
        "node-cron": "^3.0.3",
        "postgres": "^3.3.5",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-email": "^2.1.4",
        "resend": "^3.2.0",
        "response-time": "^2.3.2",
        "sharp": "^0.33.4",
        "slugify": "^1.6.6",
        "swagger-jsdoc": "^6.2.8",
        "swagger-stats": "^0.99.7",
        "swagger-ui-express": "^5.0.0",
        "uuid": "^9.0.1",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^5.0.0",
        "xendit-node": "5.0.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.7.2",
        "@commitlint/config-conventional": "^17.7.0",
        "@commitlint/cz-commitlint": "^17.7.2",
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.5",
        "@types/cookie": "^0.5.2",
        "@types/cookie-parser": "^1.4.4",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/express-status-monitor": "^1.3.3",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/morgan": "^1.9.4",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.8.3",
        "@types/node-apple-receipt-verify": "^1.7.4",
        "@types/node-cron": "^3.0.11",
        "@types/pg": "^8.10.2",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/response-time": "^2.3.8",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-stats": "^0.95.11",
        "@types/swagger-ui-express": "^4.1.6",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "autoprefixer": "^10.4.13",
        "commitizen": "^4.3.0",
        "cpx": "^1.5.0",
        "drizzle-kit": "^0.19.13",
        "eslint": "^8.42.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-html": "^7.1.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.2",
        "nodemon": "^2.0.22",
        "pm2": "^5.3.1",
        "prettier": "^3.0.0",
        "supertest": "^6.3.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3"
    },
    "apps": [
        {
            "name": "jkt48",
            "cwd": ".",
            "instances": "max",
            "script": "dist/index.js",
            "error_file": "logs/pm2/process_err.log",
            "out_file": "logs/pm2/process_out.log",
            "log_date_format": "YYYY-MM-DD HH:mm:ss.SSS",
            "max_memory_restart": "500M",
            "watch": false,
            "ignore_watch": [
                "logs",
                "node_modules",
                "public"
            ],
            "autorestart": true,
            "merge_logs": true
        }
    ]
}
